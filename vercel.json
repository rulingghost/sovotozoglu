{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "includeFiles": "backend/db.sqlite3"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/get_dollar_rate/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
